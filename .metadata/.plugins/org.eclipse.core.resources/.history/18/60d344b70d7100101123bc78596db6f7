package Bai_4_3;

import java.util.ArrayList;

public class Bai_4_3 {

	public static void addName(ArrayList<String> a, String inputName) {

		a.add(inputName);

	}

	public static void removeName(ArrayList<String> a, String inputName) {

		a.remove(inputName);
	}

	public static void changeName(ArrayList<String> a, String name, String inputName) {

		for (int i = 0; i < a.size(); i++) {

			if (a.get(i).equals(name)) {

				a.set(0, inputName);
			}
		}

	}

	public static int findName(ArrayList<String> a, String inputName) {
		for (int i = 0; i < a.size(); i++) {
			
			if(a.get(i).equals(inputName)) {
				
				return 
			}
			
		}
	}

	public static void main(String[] args) {

	}
}
